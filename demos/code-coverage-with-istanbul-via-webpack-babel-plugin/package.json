{
  "name": "code-coverage-with-istanbul-via-webpack-babel-plugin",
  "version": "0.0.0",
  "scripts": {
    "start": "npx ng serve -o",
    "test": "npx playwright test",
    "test:show-report": "npx playwright show-report ./playwright-html-report",
